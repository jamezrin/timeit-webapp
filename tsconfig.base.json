{
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom"
    ],
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "downlevelIteration": true
  }
}
