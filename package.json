{
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/typeorm/**",
      "**/typeorm",
      "**/react/**",
      "**/react"
    ]
  },
  "scripts": {
    "dev": "concurrently \"yarn run dev:*\"",
    "dev:common": "yarn workspace @timeit/common dev",
    "dev:frontend": "yarn workspace @timeit/frontend start",
    "dev:backend": "yarn workspace @timeit/backend dev",
    "prepare": "yarn workspace @timeit/common build"
  },
  "devDependencies": {
    "concurrently": "6.3.0"
  },
  "dependencies": {}
}
